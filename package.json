{
  "name": "webpack-react-starter",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "precommit": "npm run lint",
    "start": "npm run services",
    "test": "jest",
    "develop": "webpack-dev-server --hot --inline",
    "services": "NODE_PATH=./src&& nodemon -w package.json -w .babelrc -w webpack.config.js -w src -e js -e jsx -x babel-node src/services",
    "lint": "flow-typed install > /dev/null && prettier --no-color --trailing-comma all --no-semi --single-quote --write \"src/**/*.js\" \"src/**/*.jsx\" && eslint --fix src/services && flow"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-loader": "^0.1.0",
    "file-loader": "^0.11.1",
    "flow": "^0.2.3",
    "flow-bin": "^0.47.0",
    "flow-typed": "^2.1.2",
    "glob": "^7.1.2",
    "html-loader": "^0.4.5",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^20.0.4",
    "jsdom": "^11.0.0",
    "nodemon": "^1.11.0",
    "pleeease-filters": "^4.0.0",
    "postcss-calc": "^6.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.0.1",
    "postcss-custom-selectors": "^4.0.1",
    "postcss-import": "^10.0.0",
    "postcss-media-minmax": "^3.0.0",
    "postcss-nesting": "^4.0.1",
    "postcss-selector-matches": "^3.0.1",
    "postcss-selector-not": "^3.0.1",
    "prettier": "^1.4.2",
    "prop-types": "^15.5.10",
    "styled-components": "^2.0.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-flow": "^2.0.0-rc.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/fixtures/static/style.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/fixtures/static/style.js"
    }
  }
}
