{
  "name": "melody-web",
  "author": "Bailey Stoner <monokrome@monokro.me>",
  "description": "",
  "license": "MIT",
  "version": "0.1.0",
  "scripts": {
    "build": "npx cross-env NODE_ENV=production webpack -p && babel src -d lib",
    "dev": "npm run development",
    "develop": "npm run development",
    "development": "cross-env NODE_ENV=development npm run services",
    "frontend": "cross-env NODE_ENV=development npx webpack-dev-server --hot --inline",
    "lint": "standard --parser babel-eslint --plugin flowtype --fix src/**/*.js src/**/*.jsx",
    "precommit": "npm run lint",
    "production": "cross-env NODE_PATH=./src NODE_ENV=production npx babel-node src/services",
    "services": "cross-env NODE_PATH=./src npx nodemon src/services/index.js",
    "start": "cross-env NODE_ENV=development npm run services",
    "test": "npx jest --coverage ",
    "tests": "npx jest --coverage --watch --notify",
    "compose": "cross-env NODE_ENV=development bin/development-server"
  },
  "dependencies": {
    "autoprefixer": "^7.2.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bluebird": "^3.5.1",
    "cross-env": "^5.1.3",
    "express": "^4.16.2",
    "flow": "^0.2.3",
    "flow-bin": "^0.63.1",
    "flow-typed": "^2.2.3",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^11.5.1",
    "nodemon": "^1.14.11",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "styled-components": "^2.4.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "yaemit": "^2.0.0"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.15.0",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-loader": "^1.0.2",
    "file-loader": "^1.1.6",
    "glob": "^7.1.2",
    "html-loader": "^0.5.4",
    "jest": "^22.0.6",
    "jest-enzyme": "^4.0.2",
    "pleeease-filters": "^4.0.0",
    "prettier": "^1.10.2",
    "prettier-eslint-cli": "^4.7.0",
    "standard": "^10.0.3",
    "webpack-dev-server": "^2.10.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setup-tests.js",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/fixtures/static/style.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/fixtures/static/style.js"
    }
  }
}
